[user]
        #email = ckershaw@umich.edu
        email = ckershaw@google.com
	name = Carl Kershaw
[alias]
	st = status
	co = checkout
	ci = commit
	br = branch
	last = log -1 HEAD
        logt = log --date-order --all --graph --format='%C(green)%h%Creset %C(yellow)%an%Creset %C(blue bold)%ar%Creset %C(red bold)%d%Creset%s'
        logtm = log --date-order --all --graph --name-status --format='%C(green)%h%Creset %C(yellow)%an%Creset %C(blue bold)%ar%Creset %C(red bold)%d%Creset%s'
[push]
	default = simple
[core]
	editor = vim
[merge]
	tool = vimdiff
